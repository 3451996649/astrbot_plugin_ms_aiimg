{
    "api_url": {
        "description": "API 接口地址",
        "type": "string",
        "default": "https://api-inference.modelscope.cn/",
        "hint": "API服务器地址，如果你不知道你在做什么，那就默认就行。"
    },
    "api_key": {
        "description": "API 密钥",
        "type": "string",
        "hint": "请前往modelscope注册账号，并获取 API_Key",
        "default": "API_Key",
        "obvious_hint": true
    },
    "size":{
	    "description": "图片大小",
	    "type": "string",
	    "default": "1080x1920"
    },
    "model": {
        "description": "使用的模型名称",
        "type": "string",
        "default": "Qwen/Qwen-Image",
        "hint": "AI模型的名称"
    },
    "provider": {
        "description": "模型提供商，可选OpenAI或MS",
        "type": "string",
        "default": "MS",
        "hint": "可选项：OpenAI、MS"
    }
}
